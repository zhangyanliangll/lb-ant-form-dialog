{
  "name": "lb-ant-form-dialog",
  "version": "1.0.2",
  "private": false,
  "type": "module",
  "description": "基于 Ant Design Vue 4.0 的表单",
  "main": "./lib/lb-ant-form-dialog.mjs",
  "module": "./lib/lb-ant-form-dialog.js",
  "types": "packages/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "packages",
    "LICENSE",
    "lib"
  ],
  "keywords": [
    "ant",
    "vue",
    "ant-form",
    "lb-form",
    "lb-ant-form",
    "lb-ant-form-dialog",
    "ant-form-dialog"
  ],
  "bugs": {
    "url": "https://github.com/zhangyanliangll/lb-ant-form-dialog/issues"
  },
  "homepage": "https://github.com/zhangyanliangll/lb-ant-form-dialog#readme",
  "author": "zhangyanliangll",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "lib": "vite build --mode lib",
    "preview": "vite preview",
    "build": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,tsx,vue packages/** --no-error-on-unmatched-pattern --fix",
    "format": "prettier --write packages/",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "release": "release-it"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "ant-design-vue": "^4.1.2",
    "highlight.js": "^11.9.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "commitlint": "^19.2.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "release-it": "^17.1.1",
    "sass": "^1.69.5",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.8.1",
    "vue-tsc": "^2.0.6"
  },
  "release-it": {
    "github": {
      "release": false
    },
    "git": {
      "commitMessage": "release: v${version}"
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "echo 更新版本成功"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "volta": {
    "node": "18.20.1",
    "pnpm": "8.6.1",
    "yarn": "1.22.19"
  }
}
